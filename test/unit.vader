  AssertEqual IndentedParagraph(3, 1), ["V", [0, 2, 0, 0], [0, 9, 0, 0]]

Execute (inside second block):
  5
  AssertEqual IndentedParagraph(1, 0), ["V", [0, 5, 0, 0], [0, 5, 0, 0]]

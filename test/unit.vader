  AssertEqual IndentedParagraph(3, 0), ["V", [0, 1, 0, 0], [0, 10, 0, 0]]
  AssertEqual IndentedParagraph(3, 1), ["V", [0, 1, 0, 0], [0, 10, 0, 0]]

